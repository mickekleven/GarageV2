@using GarageV2.Utilities
@using static GarageV2.Utilities.Enums


<div class="col-md-4">
    <select class="form-select form-select-sm" onchange="vehicleTypeFunc(this.value);">
    @foreach (var ty in Enum.GetValues(typeof(VehicleType)))
    {
        <option class="vitype" value="@ty">@ty</option>
    }
</select>
</div>

